name_params PACKAGE
mute_pushd ${BD}/${PACKAGE}
echo "# Package \"Shinyshell-${PACKAGE}\":"
if [[ -f .README.md ]]; then
 echo
 cat .README.md
fi
echo
if [ -n ${PACKAGE} ]; then 
echo "This package contains the following functions:"
echo
for i in `list_functions`; do
 echo '<pre>'
 view_help "$i"
 echo '</pre>'
done
fi
mute_popd
