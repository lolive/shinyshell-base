name_params PACKAGE
mute_pushd ${BD}/${PACKAGE}
if [[ -f .README.md ]]; then
 cat .README.md
 echo
else
 echo "This is package "${PACKAGE}" of Shinyshell."
 echo
 echo "This package contains the following functions:"
 echo
fi
if [ -n ${PACKAGE} ]; then 
for i in `list_functions ${PACKAGE}`; do
 view_help "$i"
done
fi
mute_popd
